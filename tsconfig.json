{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": false,
    "allowJs": true,
    "sourceMap": true
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    // "noImplicitReturns": true
  },
  "exclude": ["node_modules"],
  "include": ["src", "node_modules/axios-middleware/dist"]
}
